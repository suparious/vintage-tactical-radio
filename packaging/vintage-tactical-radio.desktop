[Desktop Entry]
Version=1.0
Type=Application
Name=Vintage Tactical Radio
GenericName=SDR Radio Receiver
Comment=Military-style software defined radio application
Exec=vintage-tactical-radio %F
Icon=vintage-tactical-radio
Terminal=false
Categories=AudioVideo;Audio;HamRadio;
Keywords=radio;sdr;rtl-sdr;ham;amateur;scanner;receiver;tactical;military;
MimeType=application/x-vtr-memory;
StartupNotify=true
StartupWMClass=vintage-tactical-radio
Actions=Start;QuickTune;

[Desktop Action Start]
Name=Start Radio
Exec=vintage-tactical-radio --start

[Desktop Action QuickTune]
Name=Quick Tune to FM
Exec=vintage-tactical-radio --frequency 96900000 --mode fm
